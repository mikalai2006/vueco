{
  "name": "vueco",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vueco.umd.cjs",
  "module": "./dist/vueco.js",
  "exports": {
    ".": {
      "import": "./dist/vueco.js",
      "require": "./dist/vueco.umd.cjs"
    }
  },
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:unit-report": "vitest --environment jsdom --root src/ --reporter=json --outputFile.json=./unit-test-results.json",
    "coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-build": "vite build && vue-tsc -p tsconfig.config.json --declaration --emitDeclarationOnly && mv dist/src dist/types && npm pack",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-a11y": "^6.5.14",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.14",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vitest/coverage-istanbul": "^0.26.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "storybook_vitest_addon": "^0.0.8",
    "tailwindcss": "^3.2.1",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.8",
    "yup": "^0.32.11"
  }
}
